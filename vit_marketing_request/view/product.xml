<?xml version="1.0" ?>
<odoo>
    <data>

        <record id="vit_product_normal_form_view_spec" model="ir.ui.view">
            <field name="name">product.template.form.spec</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml"> 
                <xpath expr="//field[@name='categ_id']" position="after">
                    <field name="partner_id"/>
                    <field name="inquery_id" readonly="1"/>
                    <!-- <field name="spec"/>
                    <field name="colour"/> -->
                    <field name="sample"/>
                </xpath>
            </field>
        </record>

   </data>
</odoo>