<odoo>
	<data>
        <record id="view_vit_pak_unsur_kegiatan_formref" model="ir.ui.view">
            <field name="name">vit_pak_unsur_kegiatan_formref</field>
            <field name="model">vit.pak_unsur_kegiatan</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="vit_univ_common.view_vit_pak_unsur_kegiatan_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/div[2]/label" position="replace">
                    <label for="name" class="oe_edit_only" string="PAK Unsur Kegiatan"/>
                </xpath>
            </field>
        </record>
		<!-- action window -->
        <record id="referensi_view_employee_list" model="ir.actions.act_window">
            <field name="name">Pegawai</field>
            <field name="res_model">hr.employee</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Klik Tombol Import untuk meng-import data master pegawai
              </p>
            </field>
        </record>

        <record id="action_vit_jabatan_ref" model="ir.actions.act_window">
            <field name="name">Jabatan Fungsional</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vit.jabatan</field>
            <field name="view_mode">kanban,tree,form,graph</field>
            <field name="domain">[('fungsional','=',True)]</field>
            <field name="context">{'search_default_fieldname': 1,'default_fungsional':True}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                Click to add a new Jabatan Fungsional
                </p><p>
                Click the Create button to add a new Jabatan Fungsional
                </p>
            </field>
        </record>

        <record id="action_vit_jabatan_ref2" model="ir.actions.act_window">
            <field name="name">Jabatan Struktural</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vit.jabatan</field>
            <field name="view_mode">kanban,tree,form,graph</field>
            <field name="domain">[('struktural','=',True)]</field>
            <field name="context">{'search_default_fieldname': 1,'default_struktural':True}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                Click to add a new Jabatan Struktural
                </p><p>
                Click the Create button to add a new Jabatan Struktural
                </p>
            </field>
        </record>

        <record id="hr_leave_allocation_action_my_ref" model="ir.actions.act_window">
            <field name="name">Periode Cuti</field>
            <field name="res_model">hr.leave.allocation</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,kanban,form,activity</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Buat Periode Cuti
                </p>
            </field>
            <field name="context">{}</field>
            <field name="domain">[('employee_id.user_id', '=', uid)]</field>
        </record>
        <record id="hr_leave_allocation_action_my_ref_view_tree" model="ir.actions.act_window.view">
            <field name="sequence">1</field>
            <field name="view_mode">tree</field>
            <field name="act_window_id" ref="hr_leave_allocation_action_my_ref"/>
            <field name="view_id" ref="hr_holidays.hr_leave_allocation_view_tree_my"/>
        </record>
        <record id="hr_leave_allocation_action_my_ref_view_form" model="ir.actions.act_window.view">
            <field name="sequence">2</field>
            <field name="view_mode">form</field>
            <field name="act_window_id" ref="hr_leave_allocation_action_my_ref"/>
            <field name="view_id" ref="hr_holidays.hr_leave_allocation_view_form"/>
        </record>

        <record id="hr_leave_action_my_ref" model="ir.actions.act_window">
            <field name="name">Hak Libur</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hr.leave</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,calendar,kanban,activity</field>
            <field name="context">{}</field>
            <field name="domain">[('user_id', '=', uid)]</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Buat Hak Libur baru
                </p>
            </field>
        </record>
        <record id="hr_leave_action_my_ref_view_tree" model="ir.actions.act_window.view">
            <field name="sequence">1</field>
            <field name="view_mode">tree</field>
            <field name="act_window_id" ref="hr_leave_action_my_ref"/>
            <field name="view_id" ref="hr_holidays.hr_leave_view_tree_my"/>
