        <record id="hr_leave_action_my_ref_view_form" model="ir.actions.act_window.view">
            <field name="sequence">2</field>
            <field name="view_mode">form</field>
            <field name="act_window_id" ref="hr_leave_action_my_ref"/>
            <field name="view_id" ref="hr_holidays.hr_leave_view_form"/>
        </record>

