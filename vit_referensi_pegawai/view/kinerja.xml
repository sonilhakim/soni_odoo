        <!-- search -->
        <record id="view_vit_kinerja_penunjang_searchref" model="ir.ui.view">
            <field name="name">vit_kinerja_penunjangref</field>
            <field name="model">vit.kinerja_penunjang</field>
            <field name="inherit_id" ref="vit_bkd_inherit.view_vit_kinerja_penunjang_search"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='jenis_kegiatan_id']" position="replace">
                    <field name="jenis_kegiatan1_id" select="True"/>
                </xpath>
                <xpath expr="//search/group/filter[2]" position="replace">
                    <filter string="Jenis Kegiatan" domain="[]" name="group_by_jenis_kegiatan1_id" context="{'group_by':'jenis_kegiatan1_id'}"/>
                </xpath>
            </field>
        </record>
        <!-- kanban , to show image, please add image_small in the model -->
        <record id="view_vit_kinerja_penunjang_kanbanref" model="ir.ui.view" >
            <field name="name">vit_kinerja_penunjang</field>
            <field name="model">vit.kinerja_penunjang</field>
            <field name="inherit_id" ref="vit_bkd_inherit.view_vit_kinerja_penunjang_kanban"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='jenis_kegiatan_id']" position="replace">
                    <field name="jenis_kegiatan1_id" select="True"/>
                </xpath>
                <xpath expr="//kanban/templates" position="replace">
                    <templates>
                        <t t-name="kanban-box">
                            <div class="oe_kanban_global_click o_kanban_record_has_image_fill">
                                <div class="oe_kanban_details">
                                    <h4>
                                        <a type="open"><field name="name"/></a>
                                    </h4>
                                    <ul>

                                        <li>
                                            <span class="text-muted">Bkd</span> <field name="bkd_id"></field>
                                        </li>


                                        <li>
                                            <span class="text-muted">Jenis kegiatan</span> <field name="jenis_kegiatan1_id"></field>
                                        </li>


                                        <li>
                                            <span class="text-muted">Rekomendasi</span> <field name="rekomendasi_id"></field>
                                        </li>

                                    </ul>
                                </div>
                            </div>
                        </t>
                    </templates>
                </xpath>
            </field>
        </record>

        
