        <!-- tree view -->
        <record id="view_vit_kinerja_bidang_pendidikan_tree2ref" model="ir.ui.view">
            <field name="name">vit_kinerja_bidang_pendidikan_tree2ref</field>
            <field name="model">vit.kinerja_bidang_pendidikan</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="vit_bkd_inherit.view_vit_kinerja_bidang_pendidikan_tree2"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='jenis_kegiatan_id']" position="replace">
                    <field name="jenis_kegiatan1_id"/>
                </xpath>
                <xpath expr="//field[@name='bukti_dokumen']" position="after">
                    <field name="jenis_doc_id"/>
                    <field name="jenis_buku_id"/>
                </xpath>
                <xpath expr="//field[@name='beban_sks']" position="replace">
                    <field name="nilai_sks" string="Beban SKS"/>
                </xpath>
            </field>
        </record>
        <!-- form view -->
