<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- report qweb view -->
        <report id="report_vit_skp_menu" string="Vit - Skp"
         model="vit.skp" report_type="qweb-pdf" 
         file="vit.skp"  name="vit_skp.vit_skp_report" />

        <!-- document template -->
        <template id="vit_skp.vit_skp_report_document" >
            <t t-call="web.external_layout">
                <t t-set="doc" t-value="doc.with_context({'lang': lang})"/>
                <div class="page">
                    <h2>
                        Skp: <span t-field="doc.name"/>
                    </h2>
                    <div class="row mt32 mb32">
                        <div class="col-auto mw-100 mb-2">
                            <strong>State</strong>
                            <p class="m-0" t-field="doc.state" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Jumlah 10 sks pertama</strong>
                            <p class="m-0" t-field="doc.jumlah_10_sks_pertama" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Jumlah sks berikutnya</strong>
                            <p class="m-0" t-field="doc.jumlah_sks_berikutnya" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Tanggal</strong>
                            <p class="m-0" t-field="doc.tanggal" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Pns nip</strong>
                            <p class="m-0" t-field="doc.pns_nip" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Pns pangkat gol ruang</strong>
                            <p class="m-0" t-field="doc.pns_pangkat_gol_ruang" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Pejabat penilai nip</strong>
                            <p class="m-0" t-field="doc.pejabat_penilai_nip" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Pejabat penilai pangkat gol ruang</strong>
                            <p class="m-0" t-field="doc.pejabat_penilai_pangkat_gol_ruang" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Lokasi</strong>
                            <p class="m-0" t-field="doc.lokasi" />
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Tahun akademik</strong>
                            <p class="m-0" t-field="doc.tahun_akademik_id"/>
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Pns</strong>
                            <p class="m-0" t-field="doc.pns_id"/>
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Pejabat penilai</strong>
                            <p class="m-0" t-field="doc.pejabat_penilai_id"/>
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Pns jabatan</strong>
                            <p class="m-0" t-field="doc.pns_jabatan_id"/>
                        </div>
                        <div class="col-auto mw-100 mb-2">
                            <strong>Pns unit kerja</strong>
                            <p class="m-0" t-field="doc.pns_unit_kerja_id"/>
