                    <tbody class="rekapitulasi_jabatan_pt_ids_tbody">
                        <tr t-foreach="doc.rekapitulasi_jabatan_pt_ids" t-as="line">
                            <td><span t-field="line.name" /></td>
                            <td><span t-field="line.satuan_hasil" /></td>
                            <td><span t-field="line.jumlah_angka_kredit" /></td>
                            <td><span t-field="line.skp_id" /></td>
                        </tr> <!-- foreach-->
                    </tbody>
                </table>
                <div class="oe_structure"/>
            </t>
        </template>

        <!-- main template -->
        <template id="vit_skp.vit_skp_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-set="lang" t-value="doc.create_uid.lang"/>
                    <t t-call="vit_skp.vit_skp_report_document" />
                </t>
            </t>
        </template>

    </data>
</odoo>