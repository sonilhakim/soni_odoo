<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- tree view -->
        <record id="view_vit_kesimpulan_kinerja_dosen_line_tree" model="ir.ui.view">
            <field name="name">vit_kesimpulan_kinerja_dosen_line_tree</field>
            <field name="model">vit.kesimpulan_kinerja_dosen_line</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Kesimpulan kinerja dosen line">
                    <field name="name" />
                    <field name="syarat" />
                    <field name="kinerja" />
                    <field name="kesimpulan" />
                    <field name="kesimpulan_id" />
                </tree>
            </field>
        </record>
        <!-- form view -->
        <record id="view_vit_kesimpulan_kinerja_dosen_line_form" model="ir.ui.view">
            <field name="name">vit_kesimpulan_kinerja_dosen_line_form</field>
            <field name="model">vit.kesimpulan_kinerja_dosen_line</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Kesimpulan kinerja dosen line">
                    <header>
                    </header>
                    <sheet>
                        <div class="oe_button_box" name="button_box">
                            <!--button type="object" name="action_view_detail" class="oe_stat_button" icon="fa-pencil-square-o"-->
                                <!--field name="detail_count" widget="statinfo" string="Detail(s)"/-->
                                <!--field name="detail_ids" invisible="1"/-->
                            <!--/button-->
                        </div>
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only" string="Kesimpulan kinerja dosen line Name"/>
                            <h1><field name="name"/></h1>
                        </div>
                        <group>
                            <group>
                                <field name="syarat" />
                                <field name="kinerja" />
                                <field name="kesimpulan" />
                            </group>
                            <group>
                                <field name="kesimpulan_id" />
                            </group>
                        </group>
                        <notebook>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- search -->
        <record id="view_vit_kesimpulan_kinerja_dosen_line_search" model="ir.ui.view">
            <field name="name">vit_kesimpulan_kinerja_dosen_line</field>
            <field name="model">vit.kesimpulan_kinerja_dosen_line</field>
            <field name="arch" type="xml">
                <search string="Kesimpulan kinerja dosen line">

                    <filter string="Name" name="filter_by_name" domain="[('name','=','')]" />
                    <filter string="Syarat" name="filter_by_syarat" domain="[('syarat','=','')]" />
                    <filter string="Kinerja" name="filter_by_kinerja" domain="[('kinerja','=','')]" />
                    <filter string="Kesimpulan" name="filter_by_kesimpulan" domain="[('kesimpulan','=','')]" />
                    <field name="name" select="True"/>
                    <field name="syarat" select="True"/>
                    <field name="kinerja" select="True"/>
                    <field name="kesimpulan" select="True"/>
                    <field name="kesimpulan_id" select="True"/>
                    <group string="Group By..">
                        <filter string="Kesimpulan" domain="[]" name="group_by_kesimpulan_id" context="{'group_by':'kesimpulan_id'}"/>
                    </group>
                </search>
            </field>
        </record>
        <!-- kanban , to show image, please add image_small in the model -->
        <record id="view_vit_kesimpulan_kinerja_dosen_line_kanban" model="ir.ui.view" >
            <field name="name">vit_kesimpulan_kinerja_dosen_line</field>
            <field name="model">vit.kesimpulan_kinerja_dosen_line</field>
            <field name="arch" type="xml">
                <kanban version="7.0">
                    <field name="id" />
                    <field name="name" />
                    <field name="syarat" />
                    <field name="kinerja" />
                    <field name="kesimpulan" />
                    <field name="kesimpulan_id" />
                    <templates>
                        <t t-name="kanban-box">
                            <div class="oe_kanban_global_click o_kanban_record_has_image_fill">
                                <div class="oe_kanban_details">
                                    <h4>
                                        <a type="open"><field name="name"/></a>
                                    </h4>
                                    <ul>

                                        <li>
                                            <span class="text-muted">Kesimpulan</span> <field name="kesimpulan_id"></field>
                                        </li>

                                    </ul>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>
        <!-- calendar if a date field exists -->
        <!-- graph -->
        <record id="view_vit_kesimpulan_kinerja_dosen_line_graph" model="ir.ui.view">
            <field name="name">vit_kesimpulan_kinerja_dosen_line</field>
            <field name="model">vit.kesimpulan_kinerja_dosen_line</field>
            <field name="arch" type="xml">
                <graph string="Kesimpulan kinerja dosen line" type="bar">
                    <field name="kesimpulan_id"/>
                </graph>
            </field>
        </record>
        <!-- action window -->
        <record id="action_vit_kesimpulan_kinerja_dosen_line" model="ir.actions.act_window">
            <field name="name">Kesimpulan kinerja dosen line</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vit.kesimpulan_kinerja_dosen_line</field>
            <field name="view_type">form</field>
            <field name="view_mode">kanban,tree,form,graph</field>
            <field name="context">{"search_default_fieldname":1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                Click to add a new Kesimpulan kinerja dosen line
                </p><p>
                Click the Create button to add a new Kesimpulan kinerja dosen line
                </p>
            </field>
        </record>

        <!-- <menuitem id="menu_vit_kesimpulan_kinerja_dosen_line" name="Kesimpulan kinerja dosen line" parent="vit_bkd_config_menu" action="action_vit_kesimpulan_kinerja_dosen_line" sequence="280"/> -->
    </data>
</odoo>