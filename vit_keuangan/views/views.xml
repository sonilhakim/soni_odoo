<odoo>
  <data>

	<!-- action_Pengelolaan_COA -->
	<record id="action_Pengelolaan_COA" model="ir.actions.act_window">
		<field name="name">Pengelolaan COA</field>
		<field name="res_model">account.account</field>
		<field name="view_mode">tree,form,graph</field>
	</record>

	<!-- action_Pengelolaan_Tahun_Pembukuan -->
	<record id="action_Pengelolaan_Tahun_Pembukuan" model="ir.actions.act_window">
		<field name="name">Pengelolaan Tahun Pembukuan</field>
		<field name="res_model">account.fiscal.year</field>
		<field name="view_mode">tree,form,graph</field>
	</record>

	<!-- action_Skenario_Jurnal_Otomatis -->
	<record id="action_Skenario_Jurnal_Otomatis" model="ir.actions.act_window">
		<field name="name">Pengelolaan  Skenario Jurnal Otomatis</field>
		<field name="res_model">account.journal</field>
		<field name="view_mode">tree,form,graph</field>
	</record>

	<!-- action_Pengelolaan_Transaksi -->
	<record id="action_Pengelolaan_Transaksi" model="ir.actions.act_window">
		<field name="name">Pengelolaan Transaksi</field>
		<field name="res_model">account.move</field>
		<field name="view_mode">tree,form,graph</field>
	</record>

	<!-- action_Pengelolaan_Jurnal -->
	<record id="action_Pengelolaan_Jurnal" model="ir.actions.act_window">
		<field name="name">Pengelolaan Jurnal</field>
		<field name="res_model">account.move.line</field>
		<field name="view_mode">tree,form,graph</field>
	</record>

	<!-- menu_Jurnal_Penerimaan -->
	<record id="action_Jurnal_Penerimaan" model="ir.actions.act_window">
		<field name="name">Jurnal Penerimaan</field>
		<field name="res_model">account.invoice</field>
		<field name="view_mode">tree,form,graph</field>
		<field eval="False" name="view_id"/>
        <field name="domain">[('type','=','out_invoice')]</field>
        <field name="context">{'type':'out_invoice', 'journal_type': 'sale'}</field>
        <field name="search_view_id" ref="account.view_account_invoice_filter"/>
	</record>
	<record id="action_Jurnal_Penerimaan_view1" model="ir.actions.act_window.view">
        <field eval="1" name="sequence"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="account.invoice_tree_with_onboarding"/>
        <field name="act_window_id" ref="action_Jurnal_Penerimaan"/>
    </record>

    <record id="action_Jurnal_Penerimaan_view2" model="ir.actions.act_window.view">
        <field eval="2" name="sequence"/>
        <field name="view_mode">form</field>
        <field name="view_id" ref="account.invoice_form"/>
        <field name="act_window_id" ref="action_Jurnal_Penerimaan"/>
    </record>

	<!-- menu_Jurnal_Pengeluaran -->
	<record id="action_Jurnal_Pengeluaran" model="ir.actions.act_window">
		<field name="name">Jurnal Pengeluaran</field>
		<field name="res_model">account.invoice</field>
		<field name="view_mode">tree,form,graph</field>
		<field eval="False" name="view_id"/>
        <field name="domain">[('type','=','in_invoice')]</field>
        <field name="context">{'default_type': 'in_invoice', 'type': 'in_invoice', 'journal_type': 'purchase'}</field>
        <field name="search_view_id" ref="account.view_account_invoice_filter"/>
	</record>


	<!-- action_Laporan_Transaksi -->
	<record id="action_Laporan_Transaksi" model="ir.actions.client">
		<field name="name">Laporan Transaksi</field>
		<!-- <field name="res_model">account.move.line</field>
		<field name="view_mode">tree,form,graph</field> -->
		<field name="tag">account_report</field>
		<field name="context" eval="{'model': 'account.consolidated.journal'}" />
	</record>

	<!-- action_Laporan_Posisi_Keuangan -->
	<record id="action_Laporan_Posisi_Keuangan" model="ir.actions.client">
        <field name="name">Laporan Posisi Keuangan</field>
        <field name="tag">account_report</field>
        <field name="context" eval="{'model': 'account.financial.html.report', 'id': ref('account_reports.account_financial_report_balancesheet0')}" />
    </record>

    <!-- action_Laporan_Saldo_Master -->
    <record id="action_saldo_master" model="ir.actions.client">
        <field name="name">Saldo Master</field>
        <field name="tag">account_report</field>
        <field name="context" eval="{'model': 'account.coa.report'}" />
    </record>

	<!-- action_Laporan_Buku_Besar -->
	<record id="action_Laporan_Buku_Besar" model="ir.actions.act_window">
		<field name="name">Laporan Buku Besar</field>
		<field name="res_model">account.move.line</field>
		<field name="view_mode">tree,form,graph</field>
	</record>







    <menuitem name="Akuntansi" id="vit_keuangan_akuntansi" 
      web_icon="vit_keuangan,static/description/icon.png"
      groups="vit_keuangan.group_keuangan_user,vit_keuangan.group_keuangan_manager,vit_keuangan.group_keuangan_akuntan"/>

