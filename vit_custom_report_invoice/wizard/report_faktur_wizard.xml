<odoo>
    <data>

        <record id="report_faktur_wizard_view" model="ir.ui.view">
            <field name="name">report.faktur.wizard.form</field>
            <field name="model">report.faktur.wizard</field>
            <field name="arch" type="xml">
                <form string="Faktur">
                    <p class="oe_grey">
                        Print Faktur ?
                    </p>
                    <footer>
                        <button string="Ok" name="action_print_report" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_report_faktur_wizard_view">
            <field name="name">Print Faktur</field>
            <field name="res_model">report.faktur.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

    </data>
</odoo>