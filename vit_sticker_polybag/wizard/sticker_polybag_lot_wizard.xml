<odoo>
    <data>

        <record id="sticker_polybag_lot_wizard_view" model="ir.ui.view">
            <field name="name">sticker.polybag.wizard.form</field>
            <field name="model">sticker.polybag.wizard</field>
            <field name="arch" type="xml">
                <form string="Polybag">
                    <p class="oe_grey">
                        Print List Sticker Polybag ?
                    </p>
                    <footer>
                        <button string="Ok" name="action_print_report" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window id="action_sticker_polybag_lot_wizard_view"
            multi="True"
            key2="client_action_multi" name="Print Sticker Polybag"
            res_model="sticker.polybag.wizard" src_model="stock.production.lot"
            view_mode="form" target="new" view_type="form" />

    </data>
</odoo>