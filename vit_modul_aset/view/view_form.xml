        <record id="action_vit_referensi_aset2" model="ir.actions.act_window">
            <field name="name">Referensi</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vit.referensi_aset</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="context">{"search_default_fieldname":1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                Click to add a new Referensi aset
                </p><p>
                Click the Create button to add a new Referensi aset
                </p>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_product_usulan">
            <field name="name">Usulan Barang</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vit.product.request</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
<!--             <field name="context">{"search_default_my_request":1}</field>-->
        </record>

        <record model="ir.actions.act_window" id="act_open_vit_mutasi_view">
            <field name="name">Mutasi Barang</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vit.transfer</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="vit_asset.view_vit_transfer_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <record model="ir.actions.act_window.view" id="act_open_vit_mutasi_view_form">
            <field name="act_window_id" ref="act_open_vit_mutasi_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="vit_asset.view_vit_transfer_form"/>
        </record>

        <record model="ir.actions.act_window.view" id="act_open_vit_mutasi_view_tree">
            <field name="act_window_id" ref="act_open_vit_mutasi_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="vit_asset.view_vit_transfer_tree"/>
        </record>

        <record id="action_do_tree_all" model="ir.actions.act_window">
            <field name="name">Delivery Order</field>
            <field name="res_model">stock.picking</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,kanban,form,calendar</field>
            <field name="domain">['|',('picking_type_id.name','=','Receipts'),('picking_type_id.name','=','Penerimaan')]</field>
            <field name="context">{
                    'contact_display': 'partner_address',
            }
            </field>
            <field name="search_view_id" ref="stock.view_picking_internal_search"/>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Define a new transfer
              </p>
            </field>
        </record>
    </data>
</odoo>