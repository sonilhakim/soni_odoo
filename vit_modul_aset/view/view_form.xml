<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- form -->
        <record id="view_product_request_form" model="ir.ui.view">
            <field name="name">vit_product.request_form</field>
            <field name="model">vit.product.request</field>
            <field name="inherit_id" ref="vit_product_request.view_product_request_form"/>
            <field name="arch" type="xml">
                <button name="action_confirm" position="attributes">
                    <attribute name="groups">vit_pengelolaan_asset_data.group_pengelolaan_aset_user,vit_pengelolaan_asset_data.group_pengelolaan_aset_manager</attribute>
                </button>
                <button name="action_reject" position="attributes">
                    <attribute name="groups">vit_pengelolaan_asset_data.group_pengelolaan_aset_manager</attribute>
                </button>                    
            </field>
        </record>

        <record id="view_picking_form_department2" model="ir.ui.view">
            <field name="name">view_picking_form_department2</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='request_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

            </field>
        </record>
        <record id="vpicktree_dept" model="ir.ui.view">
            <field name="name">vpicktree_dept</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='request_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

            </field>
        </record>

        <record id="view_vit_disposal_form" model="ir.ui.view">
            <field name="name">vit.disposal.form</field>
            <field name="model">vit.disposal</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="vit_asset.view_vit_disposal_form"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <button name="action_open" position="attributes">
                    <attribute name="groups">vit_pengelolaan_asset_data.group_pengelolaan_aset_manager</attribute>
                </button>
                <!-- <button name="action_approve" position="attributes">
                    <attribute name="groups">vit_pengelolaan_asset_data.group_pengelolaan_aset_manager</attribute>
                </button>
                <button name="action_cancel" position="attributes">
                    <attribute name="groups">vit_pengelolaan_asset_data.group_pengelolaan_aset_manager</attribute>
                </button> -->
            </field>
        </record>
