<odoo>
  <data>
  		<!-- RKA Monitor tree -->
        <record id="view_rka_monitor_tree" model="ir.ui.view">
            <field name="name">anggaran_rka_monitor_tree</field>
            <field name="model">anggaran.rka</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="list rka" decoration-info="state == 'draft'" decoration-success="state == 'open'" decoration-danger="state == 'done'" decoration-muted="state == 'done'">
                    <field name="no" invisible="1"/>
                    <field name="name"/>
                    <field name="unit_id"/>
                    <field name="tahun"/>
                    <field name="period_id"/>
                    <field name="date_open"/>
                    <field name="alokasi"/>
                    <field name="anggaran"/>       
                    <field name="realisasi" />      
                    <field name="sisa" />      
                    <field name="definitif" />
                    <field name="state" />
                </tree>    
            </field>
        </record>

        <!-- RKA RENBUT tree -->
        <record id="view_rka_renbut_tree" model="ir.ui.view">
            <field name="name">anggaran_rka_renbut_tree</field>
            <field name="model">anggaran.rka</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="list rka">
                    <field name="no" invisible="1"/>
                    <field name="unit_id"/>
                    <field name="period_id" string='Periode RENBUT'/>
                    <field name="alokasi" string='Alokasi Tahun Sebelumnya'/>
                    <field name="anggaran" string='Anggaran RENBUT'/>
                    <field name="state" invisible="1"/>
                </tree>    
            </field>
        </record>

        <!-- RKA RARENJA tree -->
        <record id="view_rka_rarenja_tree" model="ir.ui.view">
            <field name="name">anggaran_rka_rarenja_tree</field>
            <field name="model">anggaran.rka</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="list rka">
                    <field name="no" invisible="1"/>
                    <field name="unit_id"/>
                    <field name="period_id" string='Periode RARENJA'/>
                    <field name="alokasi" string='Alokasi Tahun Sebelumnya'/>
                    <field name="anggaran" string='Anggaran RARENJA'/>
                    <field name="state" invisible="1"/>
                </tree>    
            </field>
        </record>

        <!-- RKA RARENJA/RENJA tree -->
        <record id="view_rka_renja_tree" model="ir.ui.view">
            <field name="name">anggaran_rka_renja_tree</field>
            <field name="model">anggaran.rka</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="list rka">
                    <field name="no" invisible="1"/>
                    <field name="unit_id"/>
                    <field name="period_id"/>
                    <field name="alokasi"/>
                    <field name="anggaran"/>
                    <field name="state" invisible="1"/>
                </tree>    
            </field>
        </record>

