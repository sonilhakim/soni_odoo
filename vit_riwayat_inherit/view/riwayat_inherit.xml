            <field name="type">form</field>
            <field name="inherit_id" ref="vit_riwayat.view_vit_riwayat_penghargaan_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='description']" position="after">
                    <field name="jenis_penghargaan"/>
                </xpath>
            </field>
        </record>

