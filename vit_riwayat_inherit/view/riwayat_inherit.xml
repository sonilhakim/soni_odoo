<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    	<!-- tree view penghargaan-->
        <record id="view_vit_riwayat_penghargaan_treeref" model="ir.ui.view">
            <field name="name">vit_riwayat_penghargaan_treeref</field>
            <field name="model">vit.riwayat_penghargaan</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="vit_riwayat.view_vit_riwayat_penghargaan_tree"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='description']" position="after">
                    <field name="jenis_penghargaan"/>
                </xpath>
            </field>
        </record>
        <!-- form view penghargaan-->
        <record id="view_vit_riwayat_penghargaan_formref" model="ir.ui.view">
            <field name="name">vit_riwayat_penghargaan_formref</field>
