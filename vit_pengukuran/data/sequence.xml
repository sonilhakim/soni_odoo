<odoo>
	<data noupdate="1">

		<record id="pengukuran_sequence" model="ir.sequence">
			<field name="name">Pengukuran Squence</field>
			<field name="code">vit.pengukuran</field>
			<field name="prefix">UK/%(month)s/%(year)s/</field>
			<field name="padding">4</field>
			<field eval="1" name="number_next"/>
			<field eval="1" name="number_increment"/>
		</record>

		<record id="pengukuran_sequence_header" model="ir.sequence">
			<field name="name">Pengukuran Squence Header</field>
			<field name="code">vit.pengukuran.header</field>
			<field name="prefix">P/%(month)s/%(year)s/</field>
			<field name="padding">4</field>
			<field eval="1" name="number_next"/>
			<field eval="1" name="number_increment"/>
		</record>
		
	</data>
</odoo>