<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_picking_form99" model="ir.ui.view">
            <field name="name">stock.picking.form99</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field eval="12" name="priority"/>
            <field name="arch" type="xml">
                <button name="button_validate" position="attributes">
                    <attribute name="groups">stock.group_stock_user,vit_pengelolaan_asset_data.group_pengelolaan_aset_manager</attribute>
                </button>
            </field>             
        </record>       
        <record id="pengadaan_form_action" model="ir.actions.act_window">
            <field name="name">Pengadaan</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">purchase.order</field>
            <field name="view_mode">tree,kanban,form,pivot,graph,calendar,activity</field>
            <field name="domain">[('state','in',('purchase', 'done'))]</field>
            <field name="context">{}</field>
            <field name="search_view_id" ref="purchase.view_purchase_order_filter"/>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Create a quotation
              </p><p>
                It will be converted into a purchase order.
              </p>
            </field>
        </record>

